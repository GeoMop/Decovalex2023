#Description: 3D test problem for testing chemical reactions
#             based on chemistry from Hammond and Lichtner, WRR, 2010

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE RICHARDS
    /
    SUBSURFACE_TRANSPORT transport
      MODE GIRT
    /
  /
END

SUBSURFACE
#=========================== numerical methods ================================
NUMERICAL_METHODS FLOW
  TIMESTEPPER
    TS_ACCELERATION 8
  /
END

NUMERICAL_METHODS TRANSPORT
END

#=========================== flow mode ========================================

#=========================== chemistry ========================================
CHEMISTRY
  PRIMARY_SPECIES
  SiO2(aq)
  Tracer
  Tracer2
  /
  MINERALS
  SiO2(am)
  /
  MINERAL_KINETICS
    SiO2(am)
      RATE_CONSTANT 1.e-10 mol/cm^2-sec
      SURFACE_AREA_VOL_FRAC_POWER 0
    /
  /
  DATABASE ./hanford.dat
  LOG_FORMULATION
  ACTIVITY_COEFFICIENTS NEWTON_ITERATION
  MOLAL
  OUTPUT
    ALL
    TOTAL
    MINERAL_SATURATION_INDEX
  /
END

REFERENCE_TEMPERATURE  15.0   # st. C

#=========================== discretization ===================================
GRID
  TYPE STRUCTURED
  ORIGIN 0.d0 0.d0 0.d0
  NXYZ 5 4 3
  DXYZ
    10. 11. 12. 13. 14.
    13. 12. 11. 10.
    15. 20. 25.
  /
END

#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
END

#=========================== datasets =========================================
#DATASET initial_U
#  HDF5_DATASET_NAME U
#  FILENAME parameters-543.h5
#END
 

DATASET initial_Calcite_vol_frac
  HDF5_DATASET_NAME SiO4(am)_volume_fraction
  FILENAME input_fields.h5
END

DATASET initial_Calcite_area
  HDF5_DATASET_NAME SiO4(am)_specific_area
  FILENAME input_fields.h5
END

#DATASET Porosity
#  FILENAME parameters-543.h5
#END

DATASET Permeability
  FILENAME input_fields.h5
END

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.25d0 #DATASET Porosity
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    PERM_X 1.d-12
    PERM_Y 1.d-12
    PERM_Z 1.d-13
  /
END

MATERIAL_PROPERTY soil2
  ID 2
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    DATASET Permeability
  /
END

MATERIAL_PROPERTY soil3
  ID 3
  POROSITY 0.25d0 #DATASET Porosity
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    DATASET Permeability
  /
END

MATERIAL_PROPERTY soil4
  ID 4
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    PERM_X 1.d-9
    PERM_Y 1.d-9
    PERM_Z 1.d-10
  /
END

#=========================== characteristic curves ============================
CHARACTERISTIC_CURVES sf1
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.286
    ALPHA  1.9401d-4
    LIQUID_RESIDUAL_SATURATION 0.115
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.286
    LIQUID_RESIDUAL_SATURATION 0.115
  /
END

#=========================== Kamil ===================================
EOS WATER                               # Zrusena stlacitelnost 
  DENSITY CONSTANT   999.75d0 kg/m^3    # 10 °C
  VISCOSITY CONSTANT 1.308d-3 Pa-s      # dynamic viscosity (10 °C)
END

#=========================== output options ===================================
OUTPUT
#  TIMES d 0.001 0.01 0.1 1. 10.
  PERIODIC TIME 10 d
  FORMAT VTK
  VARIABLES
    LIQUID_PRESSURE
    LIQUID_SATURATION
#   PERMEABILITY_X
#   PERMEABILITY_Y
#   PERMEABILITY_Z
    POROSITY
  /
 VELOCITY_AT_CENTER
END

#=========================== times ============================================
TIME
  FINAL_TIME 100.d0 d
  INITIAL_TIMESTEP_SIZE 1.d-6 d
  MAXIMUM_TIMESTEP_SIZE 10.d0 d
END

#=========================== regions ==========================================
REGION all
  COORDINATES
    0.d0 0.d0 0.d0
    60.d0 46.d0 60.d0
  /
END

REGION west
  FACE WEST
  COORDINATES
    0.d0 0.d0 0.d0
    0.d0 46.d0 60.d0
  /
END

REGION east
  FACE EAST
  COORDINATES
    60.d0 0.d0 0.d0
    60.d0 46.d0 60.d0
  /
END

#=========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
  /
  DATUM 0.d0 0.d0 0.d0
  LIQUID_PRESSURE 1.d7 ! Pa
END

FLOW_CONDITION east
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
  /
  DATUM 0.d0 0.d0 0.d0
  LIQUID_PRESSURE 1.d7 ! Pa
END

FLOW_CONDITION west
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
  /
  DATUM 0.d0 0.d0 0.d0
  LIQUID_PRESSURE 1.d7 ! Pa
END

#=========================== transport conditions =============================
TRANSPORT_CONDITION initial_groundwater
  TYPE DIRICHLET_ZERO_GRADIENT
  CONSTRAINT_LIST
    0.d0 initial_groundwater
  /
END

TRANSPORT_CONDITION groundwater
  TYPE DIRICHLET_ZERO_GRADIENT
  CONSTRAINT_LIST
    0.d0 groundwater
  /
END

#=========================== transport constraints ============================
CONSTRAINT initial_groundwater
  CONCENTRATIONS
    SiO2(aq) 1.d-30           T
    Tracer   1.e-7            T
    Tracer2  1.e-7            T
  /
  MINERALS
    SiO2(am)       DATASET initial_Calcite_vol_frac 0.1 cm^2/g  # DATASET initial_Calcite_area
#   Metatorbernite 0.    1. cm^2/cm^3
  /
END

CONSTRAINT groundwater
  CONCENTRATIONS
    SiO2(aq) 1.d-30           T
    Tracer   1.e-7            T
    Tracer2  1.e-7            T
  /
END

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial_groundwater
  REGION all
END

# west boundary condition
BOUNDARY_CONDITION west
  FLOW_CONDITION east
  TRANSPORT_CONDITION groundwater
  REGION west
END

# east boundary condition
BOUNDARY_CONDITION east
  FLOW_CONDITION east
  TRANSPORT_CONDITION groundwater
  REGION east
END


#=========================== stratigraphy couplers ============================
#STRATA
#  FILE ./543.h5
#END

STRATA
  REGION all
  MATERIAL soil1
END

END_SUBSURFACE
